{
  "manifest_version": 2,
  "name": "Save as shortcut",
  "description": "Save current page as shortcut",
  "version": "1.0.0",
  "author": "Beta Faccion",
  "browser_action": {
    "default_title": "Bear"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ]
}
