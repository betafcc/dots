#!/usr/bin/env bash
for _ARG in "$@"; do
    case "${_ARG}" in
        -v|--verbose)
            case "$PWD" in
                ${HOME}*) export _SEARCH_ROOT_PATH="${HOME}";;
                *) export _SEARCH_ROOT_PATH='/';;
            esac
    esac
done
export _fd_extra_args='--type d'


echo "${_SEARCH_ROOT_PATH}"

# exec __generic_finder
